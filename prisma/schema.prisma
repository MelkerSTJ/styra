generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Meeting {
  id          String   @id @default(cuid())
  title       String
  date        DateTime
  agenda      String?
  notes       String?
  protocol    String?
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}
